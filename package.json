{
  "name": "datariomj",
  "version": "0.2.0",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:styles": "stylelint \"{src,src/app,src/stylings}/**/*.scss\"",
    "prep": "npm run lint -- --fix && npm run lint:styles -- --fix",
    "codecov": "codecov",
    "gen:docs": "compodoc -p tsconfig.app.json src",
    "dev:cov": "ng run datariomj:serve-coverage",
    "dev:ssr": "ng run datariomj:serve-ssr",
    "serve:ssr": "node dist/datariomj/server/main.js",
    "build:ssr": "ng build --configuration production && ng run datariomj:server:production",
    "serve:pre": "http-server -c-1 dist/datariomj/browser",
    "prerender": "ng run datariomj:prerender",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "e2e": "ng e2e",
    "e2e:ci": "ng run datariomj:e2e-ci",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.4",
    "@angular/cdk": "^12.2.4",
    "@angular/common": "^12.2.4",
    "@angular/compiler": "^12.2.4",
    "@angular/core": "^12.2.4",
    "@angular/forms": "^12.2.4",
    "@angular/material": "^12.2.4",
    "@angular/platform-browser": "^12.2.4",
    "@angular/platform-browser-dynamic": "^12.2.4",
    "@angular/platform-server": "^12.2.4",
    "@angular/router": "^12.2.4",
    "@angular/service-worker": "^12.2.4",
    "@contentful/rich-text-html-renderer": "^14.1.2",
    "@nguniversal/express-engine": "^12.1.0",
    "@ngxs/devtools-plugin": "^3.7.2",
    "@ngxs/logger-plugin": "^3.7.2",
    "@ngxs/store": "^3.7.2",
    "express": "^4.15.2",
    "marked": "^2.1.3",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.4",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "^12.2.4",
    "@angular/compiler-cli": "^12.2.4",
    "@compodoc/compodoc": "^1.1.14",
    "@cypress/code-coverage": "^3.9.2",
    "@cypress/schematic": "^1.5.0",
    "@nguniversal/builders": "^12.1.0",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@types/chalk": "^2.2.0",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/express": "^4.17.11",
    "@types/jasmine": "^3.6.3",
    "@types/marked": "^2.0.0",
    "@types/node": "^12.19.16",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "check-code-coverage": "^1.10.0",
    "codecov": "^3.8.1",
    "codelyzer": "^6.0.2",
    "cypress": "^6.9.1",
    "cypress-cucumber-preprocessor": "^4.2.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "http-server": "^0.12.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "ngx-build-plus": "^12.2.0",
    "stylelint": "^13.12.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-scss": "^3.19.0",
    "ts-node": "~8.3.0",
    "typescript": "^4.2.4",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/results",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
