{
  "name": "datariomj",
  "version": "0.2.0",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:styles": "stylelint \"{src,src/app,src/stylings}/**/*.scss\"",
    "prep": "npm run lint -- --fix && npm run lint:styles -- --fix",
    "codecov": "codecov",
    "gen:docs": "compodoc -p tsconfig.app.json src",
    "dev:cov": "ng run datariomj:serve-coverage",
    "dev:ssr": "ng run datariomj:serve-ssr",
    "serve:ssr": "node dist/datariomj/server/main.js",
    "build:ssr": "ng build --configuration production && ng run datariomj:server:production",
    "serve:pre": "http-server -c-1 dist/datariomj/browser",
    "prerender": "ng run datariomj:prerender",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "e2e": "ng e2e",
    "e2e:ci": "ng run datariomj:e2e-ci",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.13",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.13",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/platform-server": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@angular/service-worker": "^16.2.12",
    "@contentful/rich-text-html-renderer": "^16.0.0",
    "@nguniversal/express-engine": "^15.2.1",
    "@ngxs/devtools-plugin": "^3.8.2",
    "@ngxs/logger-plugin": "^3.8.2",
    "@ngxs/store": "^3.8.2",
    "babel-plugin-istanbul": "^6.1.1",
    "express": "^4.18.1",
    "marked": "^4.0.16",
    "rxjs": "^6.6.7",
    "tslib": "^2.4.0",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.11",
    "@angular-eslint/builder": "^16.3.1",
    "@angular-eslint/eslint-plugin": "^16.3.1",
    "@angular-eslint/eslint-plugin-template": "^16.3.1",
    "@angular-eslint/schematics": "^16.3.1",
    "@angular-eslint/template-parser": "^16.3.1",
    "@angular/cli": "^16.2.11",
    "@angular/compiler-cli": "^16.2.12",
    "@compodoc/compodoc": "^1.1.19",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/schematic": "^1.7.0",
    "@nguniversal/builders": "^16.2.0",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@types/chalk": "^2.2.0",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.6",
    "@types/marked": "^4.0.3",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "check-code-coverage": "^1.10.0",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.2",
    "cypress": "^6.9.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "multiple-cucumber-html-reporter": "^1.21.2",
    "ngx-build-plus": "^15.0.0",
    "stylelint": "^13.13.1",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-scss": "^3.21.0",
    "ts-node": "~8.3.0",
    "typescript": "^4.9.5"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/results",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}