{
  "name": "datariomj",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:styles": "stylelint \"{src,src/app,src/stylings}/**/*.scss\"",
    "prep": "npm run lint -- --fix && npm run lint:styles -- --fix",
    "codecov": "codecov",
    "gen:docs": "compodoc -p tsconfig.app.json src",
    "dev:cov": "ng run datariomj:serve-coverage",
    "dev:ssr": "ng run datariomj:serve-ssr",
    "serve:ssr": "node dist/datariomj/server/main.js",
    "build:ssr": "ng build --configuration production && ng run datariomj:server:production",
    "serve:pre": "http-server -c-1 dist/datariomj/browser",
    "prerender": "ng run datariomj:prerender",
    "cy:open": "./node_modules/.bin/cypress run --headed",
    "cy:run": "./node_modules/.bin/cypress run",
    "e2e": "ng e2e",
    "e2e:ci": "ng run datariomj:e2e-ci",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.9",
    "@angular/cdk": "^17.0.5",
    "@angular/common": "^17.0.9",
    "@angular/compiler": "^17.0.9",
    "@angular/core": "^17.0.9",
    "@angular/forms": "^17.0.9",
    "@angular/material": "^17.0.5",
    "@angular/platform-browser": "^17.0.9",
    "@angular/platform-browser-dynamic": "^17.0.9",
    "@angular/platform-server": "^17.0.9",
    "@angular/router": "^17.0.9",
    "@angular/service-worker": "^17.0.9",
    "@contentful/rich-text-html-renderer": "^16.0.0",
    "@ngxs/devtools-plugin": "^3.8.2",
    "@ngxs/logger-plugin": "^3.8.2",
    "@ngxs/store": "^3.8.2",
    "babel-plugin-istanbul": "^6.1.1",
    "express": "^4.18.1",
    "marked": "^11.1.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.4.0",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular-eslint/builder": "^17.2.0",
    "@angular-eslint/eslint-plugin": "^17.2.0",
    "@angular-eslint/eslint-plugin-template": "^17.2.0",
    "@angular-eslint/schematics": "^17.2.0",
    "@angular-eslint/template-parser": "^17.2.0",
    "@angular/cli": "^17.0.10",
    "@angular/compiler-cli": "^17.0.9",
    "@angular/ssr": "^17.0.10",
    "@compodoc/compodoc": "^1.1.23",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/schematic": "^2.5.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/chalk": "^2.2.0",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "check-code-coverage": "^1.10.0",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.2",
    "cypress": "^13.6.2",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "http-server": "^14.1.1",
    "istanbul-lib-coverage": "^3.2.2",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "multiple-cucumber-html-reporter": "^3.6.0",
    "ngx-build-plus": "^17.0.0",
    "nyc": "^15.1.0",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-no-unsupported-browser-features": "^8.0.0",
    "stylelint-scss": "^6.0.0",
    "ts-node": "~10.9.2",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "./cypress/e2e",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/results",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
