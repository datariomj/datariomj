{
  "name": "datariomj",
  "version": "0.2.0",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:styles": "stylelint \"{src,src/app,src/stylings}/**/*.scss\"",
    "prep": "npm run lint -- --fix && npm run lint:styles -- --fix",
    "codecov": "codecov",
    "gen:docs": "compodoc -p tsconfig.app.json src",
    "dev:cov": "ng run datariomj:serve-coverage",
    "dev:ssr": "ng run datariomj:serve-ssr",
    "serve:ssr": "node dist/datariomj/server/main.js",
    "build:ssr": "ng build --configuration production && ng run datariomj:server:production",
    "serve:pre": "http-server -c-1 dist/datariomj/browser",
    "prerender": "ng run datariomj:prerender",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "e2e": "ng e2e",
    "e2e:ci": "ng run datariomj:e2e-ci",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "^13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/forms": "^13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/platform-server": "^13.1.2",
    "@angular/router": "^13.1.2",
    "@angular/service-worker": "^13.1.2",
    "@contentful/rich-text-html-renderer": "^14.1.2",
    "@nguniversal/express-engine": "^13.0.1",
    "@ngxs/devtools-plugin": "^3.7.3",
    "@ngxs/logger-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.3",
    "express": "^4.17.2",
    "marked": "^4.0.10",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "^13.1.3",
    "@angular/compiler-cli": "^13.1.2",
    "@compodoc/compodoc": "^1.1.18",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/schematic": "^1.6.0",
    "@nguniversal/builders": "^13.0.1",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@types/chalk": "^2.2.0",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/marked": "^4.0.1",
    "@types/node": "^12.20.42",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "check-code-coverage": "^1.10.0",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.2",
    "cypress": "^6.9.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "http-server": "^0.12.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "multiple-cucumber-html-reporter": "^1.18.3",
    "ngx-build-plus": "^13.0.1",
    "stylelint": "^13.13.1",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-scss": "^3.21.0",
    "ts-node": "~8.3.0",
    "typescript": "^4.5.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/results",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
